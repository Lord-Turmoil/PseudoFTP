{
    "Profile": "Default",
    "Version": "1.1.0",
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Urls": "http://*:6256",
    "ConnectionStrings": {
        "Database": "MySQL",
        "DefaultConnection": "server=<ip>;port=3306;database=Hexo;user=<user>;password=<password>"
    },
    "CorsOptions": {
        "Enable": true,
        "AllowAny": true,
        "Origins": []
    },
    "HexoOptions": {
        "Root": "<remote hexo root>"
    }
}
